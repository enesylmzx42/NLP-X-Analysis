def clean(row):
    satirlar = row["tweet"].replace("\n", " ")
    satirlar = re.sub("[0-9]+", "", satirlar) # veri iÃ§erisindeki sayÄ±lar Ã§Ä±karÄ±lÄ±yor
    satirlar = [t for t in satirlar.split()if t not in stop_words] # veri iÃ§erisindeki anlamsÄ±z kelimeler Ã§Ä±karÄ±lÄ±yor
    return " ".join(satirlar).lower().translate(str.maketrans("", "", string.punctuation))

